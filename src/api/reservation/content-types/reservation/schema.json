{
  "kind": "collectionType",
  "collectionName": "reservations",
  "info": {
    "singularName": "reservation",
    "pluralName": "reservations",
    "displayName": "Reservation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "start": {
      "type": "datetime"
    },
    "end": {
      "type": "datetime"
    },
    "car": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::car.car",
      "inversedBy": "reservations"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "stripeUrl": {
      "type": "text"
    },
    "total": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "paymentIntentId": {
      "type": "string"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "awaiting_session_complete",
        "awaiting_auth",
        "awaiting_capture",
        "completed",
        "cancelled"
      ]
    }
  }
}
